config MACH_MSM8974_G3
	bool

config MACH_MSM8974_G3_KR
	depends on ARCH_MSM8974
	select MACH_MSM8974_G3
	select LGE_MMC_SD_USE_SDCC3
	bool "MSM8974 G3 KR"
	help
	  Support for the Qualcomm MSM8974 G3 KR device.

